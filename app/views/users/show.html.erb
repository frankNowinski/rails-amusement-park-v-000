<% if @user.admin %>
  <h3><strong>ADMIN</strong></h3>
<% end %>

<h3><strong>Name: </strong><%= @user.name %></h3>
<h3><strong>Tickets: </strong><%= @user.tickets %></h3>
<h3><strong>Height: </strong><%= @user.height %> inches</h3>
<h3><strong>Mood: </strong><%= @user.mood %></h3>
<h3><strong>Nausea: </strong><%= @user.nausea %></h3>

<p><%= link_to 'See attractions', attractions_path %></p>
<p><%= link_to 'Log Out', signout_path %></p>
